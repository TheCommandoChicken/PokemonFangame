    "type": "NORMAL", // Move's type, in all uppercase.
    "pp": 35, // Base max PP
    "contact": true, // Boolean value denoting if the move makes contact.
    "priority": 0, // Priority, integer value from -6 to 6.
    "effects": // Optional list of additional effects the move has.
    [
      {
        "effect": "increased_crit" // Increases critical hit chance
      },
      {
        "effect": "multi-hit", // Move hits multiple times in a single turn
        "strike_chances": [0, 0, 35, 70, 85, 100] // Array of chances for the duration of the move. Each consecutive value is increased by the chance of that given hit occuring. In code a random value from 1-100 is checked to see which two values it sits between, and the higher of those's position in the array is the number of hits.
      },
      {
        "effect": "scatters_coins", // Causes coins to be scattered when the move connects.
        "factor": 5 // Multiplier of the user's level to determine the amount scattered
      },
      {
        "effect": "inflict_status", // Inflicts a status condition
        "target": "target", // Which Pokemon to inflict the condition upon.
        "status": "BURN", // Which condition is to be inflicted
        "chance": 10, // Percentage chance of the status being inflicted
        "overwrite_existing": false // Boolean value denoting if the status can overwrite an existing status. Has no effect for statuses which can always be stacked with others, such as Confusion.
      },
      {
        "effect": "special_accuracy" // The move's accuracy is determined by the formula (UserLevel - TargetLevel + Accuracy), bypasses accuracy and evasion checks
      },
      {
        "effect": "always_hit" // The move bypasses accuracy and evasion checks to always hit
      },
      {
        "effect": "one-hit-ko" // Guaranteed to KO the targeted Pokemon if the move succeeds
      },
      {
        "effect": "multi-turn", // Move lasts multiple turns
        "attacking_turn": 2, // Which turn the move attacks on (the turn it is used being 1)
        "length": 2, // The number of turns the move lasts
        "invulnerable": 0 // Which turn the user is invulnerable for, if any.
      },
      {
        "effect": "random_multi-turn",
        "min-length": 2,
        "max-length": 3
      },
      {
        "effect": "effects_after_move", // Effects to be applied after a multi-turn move has finished
        "effects": // Array of effects to be applied
        [
        ]
      },
      {
        "effect": "change_stat", // Modifies a stat stage
        "target": "self", // Which Pokemon to change the stages of
        "factors": [2, 0, 0, 0, 0, 0, 0, 0], // The factor by which to change each stage. Attack, Defense, Special Attack, Special Defense, Speed, Evasion, Accuracy, Crit
        "chance": 100 // Percentage chance of the stats being changed
      },
      {
        "effect": "hit_invulnerable", // Hits invulnerable Pokemon
        "moves": [19], // Which moves (by numerical ID) will have their invulnerability bypassed
        "power": 80 // The power to use when bypassing invulnerability
      },
      {
        "effect": "extended_range" // Can target Pokemon further from the user.
      },
      {
        "effect": "switch_opponent" // Forces the opponent's Pokemon to switch out at random
      },
      {
        "effect": "bypass_protect" // Bypasses Protect
      },
      {
        "effect": "trapping", // Partial trapping move, prevents the target from using most moves.
        "min-length": "4", // Minimum number of turns for the target to be trapped
        "max-length": "5" // Maximum number of turns for the target to be trapped
      },
      {
        "effect": "percent_damage", // Deals damage based on a percentage of another factor
        "damage": "target_max_health", // Which factor to use when determining the damage to deal
        "amount": 0.125 // 0-1 multiplier of the damage factor
      },
      {
        "effect": "cause_flinch", // Causes the target to flinch
        "chance": 30 // Percentage chance the target will flinch
      },
      {
        "effect": "bypass_minimize" // Bypasses evasion checks and deals double damage if the target has used Minimize this battle
      },
      {
        "effect": "crash_damage", // Deal damage to the user if the move misses
        "damage": "user_max_health", // Which factor to use when determining the damage to deal
        "amount": 0.5 // 0-1 multiplier of the damage factor
      },
      {
        "effect": "recoil", // Deal damage to the user if the move connects
        "damage": "user_max_health",
        "amount": 0.25 // 0-1 multiplier of the damage factor
      },
      {
        "effect": "same_type_immune" // Cannot affect Pokemon with the same type as the move
      },
      {
        "effect": "same_type_accuracy_bonus", // The accuracy is increased when the user has the same type as the move
        "amount": 10 // The amount by which to increase the accuracy
      },
      {
        "effect": "friendship_power", // Base power is derived from the user's friendship value, divided by the factor
        "factor": 2.5
      },
      {
        "effect": "hit_multiple", // Hit multiple Pokemon at once
        "range": 1, // Range of Pokemon from the user to target (0 = user only, 1 = adjacent, 2 = entire field)
        "opponents": true, // Whether to target opponents
        "allies": false // Whether to target allies
      },
      {
        "effect": "fail_if_underlevel" // The move will always fail if the user is lower level than the target
      },
      {
        "effect": "sound_based" // The move will fail if used on a Pokemon with the Soundproof ability
      },
      {
        "effect": "bypass_substitute" // Can hit targets even if they are behind a substitute
      },
      {
        "effect": "fixed_damage", // Deals an absolute amount of damage regardless of stats or typing.
        "amount": 20
      },
      {
        "effect": "no_effect" // Displays the message "But nothing happened!" (battle.no_effect)
      }
    ]
  },
