; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pokemon Fangame"
run/main_scene="res://test.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[autoload]

BasePokemon="*res://script/base_pokemon.gd"
Types="*res://script/types.gd"
EffectCalculation="*res://script/effect_calculation.gd"
Settings="*res://script/settings.gd"
TextManager="*res://script/text_manager.gd"
DataManager="*res://script/data_manager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[importer_defaults]

texture={
"flags/filter": false
}
3d_texture={
"compress/mode": 0,
"mipmaps/generate": false
}
2d_array_texture={
"compress/mode": 0,
"mipmaps/generate": false
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

textures/canvas_textures/default_texture_filter=0
anti_aliasing/screen_space_roughness_limiter/enabled=false
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
environment/defaults/default_environment="res://default_env.tres"
2d/snapping/use_gpu_pixel_snap=true
